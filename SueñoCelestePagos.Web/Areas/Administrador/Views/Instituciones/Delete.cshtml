@model SueñoCelestePagos.Entities.Institucion

@{
    ViewBag.Title = "Eliminar Institucion";
}

<!--BEGIN TITLE & BREADCRUMB PAGE--><!--BEGIN TITLE & BREADCRUMB PAGE-->
<h1 class="page-header">Instituciones</h1>
<ul class="breadcrumb">
    <li><span class="glyphicon glyphicon-home">&nbsp;</span>Home</li>
    <li><a href="#">Instituciones</a></li>
</ul>
<!--END TITLE & BREADCRUMB PAGE-->

<hr />

<!--BEGIN CONTENT-->
<div class="page-content">
    <div id="tab-general">
        <div class="col-md-11">
            <div class="panel">
                <div class="panel-body">
                    <h3>¿Estas seguro de querer Eliminar esta Institucion?</h3>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Nombre)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Nombre)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Cuit)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Cuit)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Domicilio)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Domicilio)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Telefono)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Telefono)
                        </dd>

                    </dl>

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-actions no-color">
                            <input type="submit" value="Eliminar" class="btn btn-default" /> |
                            @Html.ActionLink("Volver", "Index")
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>
